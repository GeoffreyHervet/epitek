#include	<iostream>

#ifndef   __EX03_H__
# define  __EX03_H__

template <typename T>
void	    print(const T &e)
{
  std::cout << e << std::endl;
}

template <typename T>
void	    foreach(const T *tab, void (*f)(const T&), size_t size)
{
  size_t    idx = 0;

  while (idx < size)
    f(tab[idx++]);
}


#endif     /* !__EX03_H__ */
